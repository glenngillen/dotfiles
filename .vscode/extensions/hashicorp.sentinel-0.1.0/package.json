	},
	"bugs": {
		"url": "https://github.com/hashicorp/vscode-sentinel/issues"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"devops",
		"sentinel",
		"hcl"
	],
	"contributes": {
		"languages": [
			{
				"id": "sentinel",
				"aliases": [
					"HashiCorp Sentinel",
					"sentinel"
				],
				"extensions": [
					".sentinel"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "sentinel",
				"scopeName": "source.sentinel",
				"path": "./syntaxes/sentinel.tmlanguage.json"
			}
		]
	},
	"scripts": {
		"package": "vsce package",
		"test": "npm run test:syntax",
		"test:syntax": "npm run test:grammar && npm run test:snap",
		"test:grammar": "npx vscode-tmgrammar-test -s source.sentinel -g syntaxes/sentinel.tmlanguage.json -t tests/unit/**/*.sentinel",
		"test:snap": "npx vscode-tmgrammar-snap -s source.sentinel -g syntaxes/sentinel.tmlanguage.json -t tests/snapshot/**/*.sentinel"
	},
	"devDependencies": {
		"vsce": "^2.6.6",
		"vscode-tmgrammar-test": "^0.0.11"
	},
